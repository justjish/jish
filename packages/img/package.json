{
  "name": "@jish/img",
  "type": "module",
  "source": "/src/img.ts",
  "main": "./img.js",
  "types": "./img.d.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=6.0.0"
  },
  "exports": {
    ".": {
      "types": "./img.d.ts",
      "import": "./img.js",
      "require": "./img.cjs",
      "default": "./img.js"
    },
    "./cfworker.js": {
      "types": "./cfworker.d.ts",
      "import": {
        "types":"./cfworker.d.mts",
        "default":"./cfworker.mjs"
      },
      "require":{
        "types": "./cfworker.d.cts",
         "default": "./cfworker.cjs"
      },
      "default": "./cfworker.js"
    },
    "./node.js": {
      "types": "./node.d.mts",
      "import": {
        "types":"./node.d.mts",
        "default":"./node.mjs"
      },
      "require": {
        "types": "./node.d.cts",
        "default": "./node.cjs"
      },
      "node": "./node.js"
    },
    "./pure.js": {
      "types": "./pure.d.ts",
      "import": {
        "types":"./pure.d.ts",
        "default":"./pure.js"
      },
      "require": {
        "types": "./pure.d.cts",
        "default": "./pure.cjs"
      },
      "default": "./pure.js"
    },
    "./react.js": {
      "types": "./react.d.ts",
      "import": {
        "types":"./react.d.ts",
        "default":"./react.js"
      },
      "require": {
        "types": "./react.d.cts",
        "default":"./react.cjs"
      },
      "default": "./react.js"
    },
    "./package.json": "./package.json",
    "./img.css": "./img.css"
  },
  "imports": {
    "#dep": {
      "node": "@next-boost/hybrid-disk-cache"
    }
  },
  
  "version": "0.0.0",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist",
    "img.css"
  ],
  "scripts": {
    "dev": "microbundle lib/*.ts --watch",
    "build": "nanobundle build",
     "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue src"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@cloudflare/workers-types": "^4.20230214.0",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "clsx": "^1.2.1",
    "is-svg": "^4.3.2",
    "js-image-lib": "^1.0.1",
    "lru-cache": "^7.15.0",
    "mime-tree": "^0.1.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@jish/cloudflare-env": "workspace:*",
    "@jish/eslint-config": "workspace:*",
    "@jish/tsconfig": "workspace:*",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.35",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "microbundle": "^0.15.1",
    "nanobundle": "^1.5.0",
    "rollup-plugin-node-externals": "^5.1.2",
    "typescript": "4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.3"
  }
}
